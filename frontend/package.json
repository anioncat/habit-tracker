{
  "name": "habit-tracker-bun",
  "private": true,
  "version": "0.5.0",
  "schemaVersion": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"tailwind -i src/app.css -o public/appStyle.css --watch\" \"vite --host 0.0.0.0 --port 1234\"",
    "build": "tsc && tailwind -i src/app.css -o public/appStyle.css --minify && vite build && mkdir -p ../release && rm -f ../release/frontend-release.tar.gz && tar -czf ../release/frontend-release.tar.gz dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "dayjs": "^1.11.10",
    "immer": "^10.0.3",
    "lodash": "^4.17.21",
    "lucide-react": "^0.379.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.2",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.5",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
